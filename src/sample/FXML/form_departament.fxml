<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="156.0" prefWidth="242.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FormDepartament">
   <children>
      <TextField fx:id="editText" layoutX="11.0" layoutY="53.0" prefHeight="25.0" prefWidth="220.0" />
      <Label layoutX="15.0" layoutY="28.0" prefHeight="25.0" prefWidth="198.0" text="Название департамента">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Button fx:id="btnSave" layoutX="14.0" layoutY="95.0" mnemonicParsing="false" onAction="#saveAction" text="Сохранить">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnCancel" layoutX="128.0" layoutY="95.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="37.0" prefWidth="100.0" text="Отмена">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
